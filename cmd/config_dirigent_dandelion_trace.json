{
  "Seed": 42,

  "Platform": "Dirigent",
  "EndpointPort": 80,

  "DirigentControlPlaneIP": "127.0.0.1:9091",

  "TracePath": "data/traces/100",
  "Granularity": "minute",
  "OutputPathPrefix": "data/out/experiment_azure",
  "IATDistribution": "exponential",
  "CPULimit": "1vCPU",
  "ExperimentDuration": 2,
  "WarmupDuration": 0,

  "IsPartiallyPanic": false,
  "EnableZipkinTracing": false,
  "EnableMetricsScrapping": false,
  "MetricScrapingPeriodSeconds": 15,
  "AutoscalingMetric": "concurrency",

  "GRPCConnectionTimeoutSeconds": 15,
  "GRPCFunctionTimeoutSeconds": 180
}
